keywords = {basic reproduction ratio,epidemiology,infectious diseases,reservoirs of infection},
number = {1522},
pages = {1359--1364},
pmid = {12965026},
title = {{A new method for estimating the effort required to control an infectious disease}},
url = {http://rspb.royalsocietypublishing.org/cgi/doi/10.1098/rspb.2003.2339},
