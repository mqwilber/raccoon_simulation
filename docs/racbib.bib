@article{Restif2012,
abstract = {Infectious disease ecology has recently raised its public profile beyond the scientific community due to the major threats that wildlife infections pose to biological conservation, animal welfare, human health and food security. As we start unravelling the full extent of emerging infectious diseases, there is an urgent need to facilitate multidisciplinary research in this area. Even though research in ecology has always had a strong theoretical component, cultural and technical hurdles often hamper direct collaboration between theoreticians and empiricists. Building upon our collective experience of multidisciplinary research and teaching in this area, we propose practical guidelines to help with effective integration among mathematical modelling, fieldwork and laboratory work. Modelling tools can be used at all steps of a field-based research programme, from the formulation of working hypotheses to field study design and data analysis. We illustrate our model-guided fieldwork framework with two case studies we have been conducting on wildlife infectious diseases: plague transmission in prairie dogs and lyssavirus dynamics in American and African bats. These demonstrate that mechanistic models, if properly integrated in research programmes, can provide a framework for holistic approaches to complex biological systems.},
author = {Restif, Olivier and Hayman, David T.S. and Pulliam, Juliet R.C. and Plowright, Raina K. and George, Dylan B. and Luis, Angela D. and Cunningham, Andrew A. and Bowen, Richard A. and Fooks, Anthony R. and O'Shea, Thomas J. and Wood, James L.N. and Webb, Colleen T.},
doi = {10.1111/j.1461-0248.2012.01836.x},
file = {:Users/mqwilber/Dropbox/Documents/Papers{\_}and{\_}Books/Restifetal2012{\_}EcoLett.pdf:pdf},
isbn = {1461-0248},
issn = {1461023X},
journal = {Ecology Letters},
keywords = {Field ecology,Infectious diseases,Mathematical models,Statistical models,Study design,Wildlife epidemiology},
number = {10},
pages = {1083--1094},
pmid = {22809422},
title = {{Model-guided fieldwork: Practical guidelines for multidisciplinary research on wildlife ecological and epidemiological dynamics}},
volume = {15},
year = {2012}
}
