issn = {0962-8452},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {a trade-,animal dispersion,as bene,functional response,helminth parasites,hypothesis that maximizing the,involves costs as well,johnstone 1998 a,rate of energy intake,shorebirds,that is there is,these studies are consistent,ts,with the},
number = {1429},
pages = {1703--1709},
pmid = {10501039},
title = {{A trade-off between energy intake and exposure to parasites in oystercatchers feeding on a bivalve mollusc}},
url = {http://rspb.royalsocietypublishing.org/cgi/doi/10.1098/rspb.1999.0835},
volume = {266},
year = {1999}
}
